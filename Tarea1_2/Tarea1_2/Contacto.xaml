<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Tarea1_2.Contacto">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Ejercicio 1.2 PGM II" VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand" />
            <Label x:Name="lblnombre" Text="{Binding nombre}" VerticalOptions="Center" HorizontalOptions="CenterAndExpand" />
            <Label x:Name="lblapellido" Text="{Binding apellido}" VerticalOptions="Center" HorizontalOptions="CenterAndExpand" />
            <Label x:Name="lbledad" Text="{Binding edad}" VerticalOptions="Center" HorizontalOptions="CenterAndExpand" />
            <Label x:Name="lblcorreo" Text="{Binding correo}" VerticalOptions="Center" HorizontalOptions="CenterAndExpand"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>